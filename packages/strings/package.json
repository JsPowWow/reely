{
  "name": "@reely/strings",
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "https://github.com/JsPowWow/reely"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@reely/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "vite": "^7.0.0",
    "@reely/utils": "*"
  },
  "nx": {
    "tags": [
      "scope:strings"
    ],
    "targets": {
      "build-base": {
        "command": "echo 'Running build-base for strings. This can be replaced with arbitrary commands.'",
        "cache": true
      },
      "build": {
        "dependsOn": [
          "build-base"
        ]
      }
    }
  }
}
